    "import sys\n",
    "A, B, C = input().split()\n",
    "A = int(A)\n",
    "B = int(B)\n",
    "C = int(C)\n",
    "if C <= B :\n",
    "    print(-1)\n",
    "    sys.exit()\n",
    "k = A / (C-B)\n",
    "if type(k) == int:\n",
    "    k = k+1 \n",
    "if type(k) == float:\n",
    "    k = int(k) + 1\n",
    "print(k)\n",
    "\n"
   ]
  }
